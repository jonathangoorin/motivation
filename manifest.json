{
   "manifest_version": 3,
   "name": "Motivation",
   "version": "1.0",
   "description": "Replace new tab page with Motivation",
   "background": {
      "service_worker": "motivation.js"
   },
   "chrome_url_overrides": {
      "newtab": "dashboard.html"
   },
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
   }
}
